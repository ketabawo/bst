%section#result_list
  %h1
    %i.fa.fa-database
    その他ラウンド大会結果
  #result_list_inner
    %dl.s2016.on_sub
      %dt 2016年
      %dd
        %ul
          %li
            - (1..6).each do |i|
              = link_to "第#{i}戦", "/results/2016/round#{i}.html"
    %dl.s2015.on_sub
      %dt 2015年
      %dd
        %ul
          %li
            - (1..5).each do |i|
              = link_to "第#{i}戦", "/results/2015/round#{i}.html"
            = link_to "EX", "/results/2015/roundex.html"
    %dl.s2014.on_sub
      %dt 2014年
      %dd
        %ul
          - (1..5).each do |i|
            %li= link_to "第#{i}戦", "/results/2014/round#{i}.html"
    %dl.s2013.on_sub
      %dt 2013年
      %dd
        %ul
          - (1..4).each do |i|
            %li= link_to "第#{i}戦", "/results/2013/round#{i}.html"
